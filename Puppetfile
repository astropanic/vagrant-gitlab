# Puppetforge modules
forge "http://forge.puppetlabs.com"

mod "apt",
    :git => "git://github.com/sbadia/gitlab-apt.git"
mod "gitlab",
    :git => "git://github.com/sbadia/puppet-gitlab.git"
mod "redis",
    :git => "git://github.com/fsalum/puppet-redis.git"
mod "nginx",
    :git => "git://github.com/jfryman/puppet-nginx.git"
